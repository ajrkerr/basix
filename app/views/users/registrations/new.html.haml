%article 
  %h2 Register

  = semantic_form_for(resource, :as => resource_name, :url => registration_path(resource_name)) do |f| 
    = devise_error_messages! 

    = f.input :first_name
    = f.input :last_name
    = f.input :email 
    = f.input :password 
    = f.input :password_confirmation 
    = f.submit "Register"

  = render :partial => "users/shared/links" 
